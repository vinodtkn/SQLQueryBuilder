    "/selectQuery": {
      "post": {
        "tags": [
          "query-builder-controller"
        ],
        "summary": "getSelectQuery",
        "operationId": "getSelectQueryUsingPOST",
        "produces": [
          "text/plain"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "queryComponents",
            "description": "queryComponents",
            "required": true,
            "schema": {
              "$ref": "#/definitions/QueryComponents"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "type": "string"
            }
          },
          "201": {
            "description": "Created"
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "Not Found"
          }
        }
      }
