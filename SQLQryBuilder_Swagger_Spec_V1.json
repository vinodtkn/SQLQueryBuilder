          "$ref": "#/definitions/QueryParameters"
        }
      },
      "title": "QueryComponents"
    },
    "QueryParameters": {
      "type": "object",
      "properties": {
        "groupByColumns": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/GroupByConditionParameters"
          }
        },
        "joinConditions": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/JoinConditionParameters"
          }
        },
        "orderByConditions": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/OrderByConditionParameters"
          }
        },
        "selectionColumns": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/SelectionColumnParameters"
          }
        },
        "tableAliases": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "tableNames": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "whereConditions": {
          "$ref": "#/definitions/WhereConditionDetails"
        }
      },
      "title": "QueryParameters"
    },
    "SelectionColumnParameters": {
      "type": "object",
      "properties": {
        "alias": {
          "type": "string"
        },
        "columns": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      },
      "title": "SelectionColumnParameters"
    },
    "SubQueryDetails": {
      "type": "object",
      "properties": {
        "queryComponents": {
          "$ref": "#/definitions/QueryParameters"
        }
      },
      "title": "SubQueryDetails"
    },
    "View": {
      "type": "object",
      "properties": {
        "contentType": {
          "type": "string"
        }
      },
      "title": "View"
    },
    "WhereConditionDetails": {
      "type": "object",
      "properties": {
        "conditionAppender": {
          "type": "string"
        },
        "conditions": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/WhereConditionParameters"
          }
        },
        "whereConditionParam": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/WhereConditionParameters"
          }
        }
      },
      "title": "WhereConditionDetails"
    },
