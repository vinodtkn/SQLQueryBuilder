  "definitions": {
    "GroupByConditionParameters": {
      "type": "object",
      "properties": {
        "alias": {
          "type": "string"
        },
        "columns": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      },
      "title": "GroupByConditionParameters"
    },
    "JoinConditionParameters": {
      "type": "object",
      "properties": {
        "joinType": {
          "type": "string"
        },
        "primaryTableColumnName": {
          "type": "string"
        },
        "secondaryTableAliasName": {
          "type": "string"
        },
        "secondaryTableColumnName": {
          "type": "string"
        }
      },
      "title": "JoinConditionParameters"
    },
