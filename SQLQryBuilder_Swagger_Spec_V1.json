      "type": "object",
      "properties": {
        "comparableColumn": {
          "type": "string"
        },
        "comparableTableAlias": {
          "type": "string"
        },
        "comparingAlias": {
          "type": "string"
        },
        "comparingColumn": {
          "type": "string"
        },
        "comparingOperator": {
          "type": "string"
        },
        "comparingValue": {
          "type": "string"
        },
        "comparingWithColumn": {
          "type": "boolean"
        },
        "comparingWithSubQuery": {
          "type": "boolean"
        },
        "comparingWithValue": {
          "type": "boolean"
        },
        "subQueryDetails": {
          "$ref": "#/definitions/SubQueryDetails"
        }
      },
      "title": "WhereConditionParameters"
